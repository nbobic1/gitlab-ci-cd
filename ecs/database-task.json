{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 3306,
          "protocol": "tcp",
          "containerPort": 3306
        }
      ],
      "cpu": 256,
      "environment": [
        {
          "name": "MYSQL_DATABASE",
          "value": "DBWT19"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "${MYSQL_ROOT_PASSWORD}"
        }
      ],
      "memory": 512,
      "image": "mysql",
      "healthCheck": {
        "retries": 10,
        "command": [
          "mysqladmin ping -h localhost"
        ],
        "timeout": 20,
        "interval": 30
      },
      "name": "database"
    }
  ],
  "placementConstraints": [
    {
      "type": "memberOf",
      "expression": "attribute:ecs.subnet-id in [$PRIVATE_SUBNET_ID]"
    }
  ],
  "family": "database-task"
}
