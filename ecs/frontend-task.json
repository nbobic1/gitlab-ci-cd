{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "cpu": 256,
      "environment": [
        {
          "name": "MYSQL_DB",
          "value": "${MYSQL_DB}"
        }
      ],
      "memory": 512,
      "image": "kibrovic/frontend-task",
      "name": "frontend-task"
    }
  ],
  "placementConstraints": [
    {
      "type": "memberOf",
      "expression": "attribute:ecs.subnet-id in [subnet-003bbf6e003a5404c]"
    }
  ],
  "family": "frontend-task"
}
